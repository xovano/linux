/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2835";



	fragment@0 {
		target = <&spidev0>;

		__overlay__ {
			status = "disabled";
		};
	};

	fragment@1 {
		target = <&spi0>;

		__overlay__{
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			ad7293@0{
				compatible = "adi,ad7293";
				reg = <0>;
				spi-max-frequency = <1000000>;
			};
		};
	};
}; 